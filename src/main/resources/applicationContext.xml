<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
               http://www.springframework.org/schema/context
               http://www.springframework.org/schema/context/spring-context.xsd">


    <context:property-placeholder  location="classpath:data.properties"/>

    <bean id="user"
          class="com.fact.springapp.di.User"
          init-method="doStar"
          destroy-method="doEnd">
        <property name="personInjection" ref="person"></property>
        <property name="email" value="${us.Email}"></property>
        <property name="team" value="${us.Team}"></property>
    </bean>


    <bean id="person" class="com.fact.springapp.di.Person">
    </bean>
<!--   <bean id="mySport" class="com.fact.springapp.ioc.Football">-->
<!--   </bean>-->

</beans>